{
  "name": "dethercontractrepo",
  "version": "2.1.0",
  "description": "Dether provides a decentralized peer-to-peer ether network that enables anyone on Earth to buy ether with cash and spend it at physical stores nearby.",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node_modules/.bin/truffle test",
    "trace": "MODE=trace node_modules/.bin/truffle test",
    "testKovanInfura": "node_modules/.bin/truffle test --network kovanInfura",
    "compile": "node_modules/.bin/truffle compile",
    "migrate": "node_modules/.bin/truffle migrate --reset",
    "migrateKovan": "node_modules/.bin/truffle migrate --network kovan --reset",
    "esdoc": "./node_modules/.bin/esdoc",
    "security": "./node_modules/.bin/truffle run verify Zone"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dethertech/dethercontracts.git"
  },
  "author": "Dether's team <hello@dether.io> (https://github.com/dethertech)",
  "contributors": [
    "Dether team <hello@dether.io> "
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dethertech/dethercontracts/issues"
  },
  "dependencies": {
    "bignum": "^0.13.0",
    "ethereum-datetime": "^1.0.0",
    "ethereumjs-util": "^5.2.0",
    "openzeppelin-solidity": "^2.1.2",
    "truffle": "^5.0.33",
    "truffle-hdwallet-provider": "^1.0.0-web3one.5",
    "truffle-privatekey-provider": "^1.5.0",
    "web3": "1.2.1"
  },
  "devDependencies": {
    "bignumber.js": "^8.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bn": "^0.1.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "ethereumjs-abi": "^0.6.4",
    "ethers": "^2.1.3",
    "for-async": "^1.0.1",
    "fs": "0.0.1-security",
    "papaparse": "^4.6.3",
    "pretty-time": "^1.1.0",
    "truffle-assertions": "^0.8.2",
    "truffle-security": "git+https://github.com/ConsenSys/truffle-security.git"
  }
}
