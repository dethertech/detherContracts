{
  "name": "dethercontract",
  "version": "2.0.0",
  "description": "Dether provides a decentralized peer-to-peer ether network that enables anyone on Earth to buy ether with cash and spend it at physical stores nearby.",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node_modules/.bin/truffle test",
    "testKovanInfura": "node_modules/.bin/truffle test --network kovanInfura",
    "migrate": "node_modules/.bin/truffle migrate --reset",
    "migrateKovanInfura": "node_modules/.bin/truffle migrate --network kovanInfura --reset",
    "esdoc": "./node_modules/.bin/esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dethertech/dethercontracts.git"
  },
