    "url": "https://github.com/dethertech/dethercontracts/issues"
  },
  "dependencies": {
    "@digix/truffle-lightwallet-provider": "^0.1.2",
    "esdoc": "^1.0.4",
    "truffle-hdwallet-provider": "0.0.3",
    "web3-eth-abi": "^1.0.0-beta.30",
    "zeppelin-solidity": "1.6.0"
  },
  "devDependencies": {
    "@digix/doxity": "^0.5.2",
    "dotenv": "^4.0.0",
    "ethereumjs-abi": "^0.6.4",
